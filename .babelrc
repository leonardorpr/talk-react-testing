{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": [
          "./src"
        ],
        "extensions": [
          ".js"
        ],
        "alias": {
          "screens": "./src/screens",
          "components": "./src/components",
          "store": "./src/store",
          "assets": "./src/assets",
          "specs": "./src/specs"
        }
      }
    ]
  ]
}
